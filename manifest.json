{
    "manifest_version": 2,
    "name": "Bloc Supportfu Helper",
    "description": "A plugin that allows a Bloc admin to interact with supportfu easily.",
    "version": "1.0",
    "permissions": [
        "https://www.bloc.io"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["https://bloc.supportfu.com/*"],
            "js": ["jquery-2.0.3.min.js", "bloc_supportfu.js"]
        }
    ]
}
